<link rel="stylesheet" href="/searchPage.css">
<link rel="stylesheet" href="/style.css"> 

<div id="climbInfo">
<h1>{{climb.name}}</h1>
<h3>Grade: {{climb.difficulty_yd.name}}</h3>
<p>Length: {{climb.length}} meters</p>
<p>Coordinates:
  <a
    href="https://www.google.com/maps?q={{climb.coodinates}}"
    target="_blank"
  >{{climb.coordinates}}</a></p>
  <p id="error-msg"></p>
<button id="toDoButton" data-id="{{climb.id}}" display="false">Add To List</button><br />
<img src="{{climb.photo}}" alt="{{climb.name}}" style="max-width: 100%; height: auto;" />
<p>Comments:</p>

<p></p>
<button id="addCommentButton">Add A Comment</button>
</div>

<script type="module" src="/scripts/climb.js"></script>
