<link rel="stylesheet" href="/css/profile.css">
<script src="https://code.jquery.com/jquery-3.7.1.js"></script>
<script src="https://code.jquery.com/ui/1.13.3/jquery-ui.js"></script>
<script src="https://d3js.org/d3.v7.min.js"></script>

<div id="containerOne">
    <div id="pbContainer">
        <div id="pbHeader">
            <h2>Basic Info</h2>
        </div>
        <div id="pbBody">
                <div id="profilePic">
                    <img src="/images/circlepic.jpg" alt="profilePic">
                </div>
                <div id="basicInfo">
                    <p id="user-name">{{user.first_name}} {{user.last_name}}</p>
                    <p id="user-email">{{user.email}}</p>
                </div>
        </div>
    </div>
</div>
<div id="containerTwo">
        <div id="completeList">
        <div id="completeHeader">
            <h2>Completed Climbs</h2>
        </div>
        <div id="completeBody">
        </div>
    </div>
        <div id="barGraph">
        <div id="barHeader">
            <h2>Grade Stats</h2>
        </div>
        <div id="barBody">

        </div>
    </div>
</div>

<script type="module" src="/scripts/user.js"></script>